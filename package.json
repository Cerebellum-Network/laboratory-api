{
  "name": "@Cerebellum-Network/laboratory-api",
  "repository": "git://github.com/Cerebellum-Network/laboratory-api.git",
  "version": "1.17.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "init": "npm i",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "prestart:dev": "npm run typeorm:migration:run",
    "prestart:prod": "npm run typeorm:migration:run",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon --exec \"npm run lint && ts-node src/main.ts\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "test": "jest",
    "pretest": "npm run lint",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/modules/database/ormconfig.ts",
    "typeorm:migration:create": "npm run typeorm -- migration:create -n",
    "typeorm:migration:run": "npm run typeorm -- migration:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretest"
    }
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/typeorm": "^7.1.5",
    "@polkadot/api": "^4.2.1",
    "@polkadot/types": "^4.2.1",
    "@polkadot/util": "^6.0.5",
    "@polkadot/util-crypto": "^6.0.5",
    "@types/better-sqlite3": "^5.4.1",
    "@types/node": "^14.11.1",
    "@types/sprintf-js": "^1.1.2",
    "axios": "^0.21.1",
    "better-sqlite3-helper": "^3.1.2",
    "bn": "^1.0.5",
    "bn.js": "^5.1.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jshint": "^2.12.0",
    "moment": "^2.29.1",
    "nestjs-rate-limiter": "^2.7.0",
    "pg": "^8.5.1",
    "promise-deferred": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sprintf-js": "^1.1.2",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.31",
    "web3": "^1.5.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.4",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^3.1.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
