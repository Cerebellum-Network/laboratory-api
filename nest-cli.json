{
  "collection": "@nestjs/schematics",
  "language": "ts",
  "sourceRoot": "api/src",
  "monorepo": true,
  "root": "api",
  "projects": {
    "api": {
      "type": "application",
      "root": "api",
      "entryFile": "main",
      "sourceRoot": "api/src",
      "compilerOptions": {
        "tsConfigPath": "api/tsconfig.api.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "api/tsconfig.api.json"
  }
}
